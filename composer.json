{
    "name": "hwpay/app",
    "description": "HWPay application",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.5",
        "baldinof/roadrunner-bundle": "^3.1",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/orm": "^3.4",
        "symfony/console": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/yaml": "^8.0",
        "temporal/sdk": "^2.16"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.0",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/maker-bundle": "^1.52",
        "symfony/phpunit-bridge": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": true,
        "sort-packages": true,
        "audit": {
            "ignore": [
                "PKSA-xbp4-qwr2-cbjj"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/console cache:clear --no-warmup 2>/dev/null || true"
        ],
        "post-update-cmd": [
            "php bin/console cache:clear --no-warmup 2>/dev/null || true"
        ],
        "test": "bin/phpunit",
        "test:unit": "bin/phpunit tests/Unit",
        "test:integration": "bin/phpunit tests/Integration"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": ">=8.0"
        }
    }
}